<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.univali.prog.healthcheck">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListarConsulta"></activity>
        <activity android:name=".ListarPaciente" />
        <activity android:name=".ListarMedico" />
        <activity android:name=".EditaConsulta" />
        <activity android:name=".EditaPaciente" />
        <activity android:name=".EditaMedico" />
        <activity android:name=".AdicionaConsulta" />
        <activity android:name=".AdicionaPaciente" />
        <activity android:name=".AdicionaMedico" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>